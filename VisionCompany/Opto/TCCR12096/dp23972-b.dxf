<script type="text/javascript" language="Javascript">
	var $policyObj;
	$(document).ready(function(){        
		$('#generalSubmit').click(function(){
			//$(formame).submit();
			//console.log($(formame).serialize());
			$policyObj=new Object({});
			$submail=$('#form_email').val();
			if(($submail.length > 0) && ($submail.indexOf('@') !== -1) && ($submail.indexOf('.') !== -1) ){
				//console.log($('#form_email').val());
				$policyObj.email=$('#form_email').val();
				$('.oe-conditions').fadeOut(400);
			}else{
				$('.oe-conditions').fadeIn(500);
				return false;
			}
			
			if(typeof $('#form_policy') !== 'undefined'){
				$policyObj.policy=$('#form_policy').prop('checked');	
			}
			
			if(typeof $('#form_newsletter') !== 'undefined'){
				$policyObj.newsletter=$('#form_newsletter').prop('checked');	
			}
			
			if(typeof $('#form_catalog') !== 'undefined'){
				$policyObj.catalog=$('#form_catalog').prop('checked');	
			}
			
			if($('.list_country').length !== 0){
				$policyObj.lang=$('.list_country option:selected').val();
			}else{
				$policyObj.lang=$(myformsubmit + ' .country option:selected').val();
			}
			
			subscribepolicy($policyObj);
	});
		
		$('#form_policy').change(function(){					
			if($(this).prop('checked') === true) {
				$('.submit_container').fadeIn(400);
			}else{
				$('.submit_container').fadeOut(500);
			}					
		});
    });        
    
	var myformsubmit;
    
	function policypop(args,myform,inputmail) {
			if (typeof inputmail !== "undefined"){
				$('#form_email').val(inputmail);
			}
									
			if (typeof myform !== "undefined"){
				myformsubmit =myform;
			}						
			
            $.magnificPopup.open({
                items: {src: args },
				focus: '#form_policy',
                type: 'inline',
				preloader: false,
				callbacks: {
					beforeClose: function() {
						$(myform).find("input[name='privacycondition']").val(encodeURIComponent(JSON.stringify($policyObj)));
					},
					afterClose:function(){						
						$(myform).submit();
						tagga();
					}
				}
            });
						
    }
	

	function subscribepolicy($polObj){
		//console.log($polObj);
		//return false;
		$.ajax({
		  beforeSend: function(){$('.esito').addClass("loading");},
          url: '/message/subscribe_newsletter',
          data : { param : $polObj },
          dataType: 'text',
          type : 'post',
		  complete: function(){$('.esito').removeClass("loading");}
      })
      
      .done(function( msg ) {
        //console.log(msg);
         var Myobj = jQuery.parseJSON( msg );
		 $('.esito').append(Myobj.msg);			
		 //if(Myobj.result === 1 ){
			$('.esito').fadeOut(1500,function(){
				$.magnificPopup.instance.close();
			});
		// }else{
		//	$.magnificPopup.instance.close();
		// }
		 
      }); 
		
	}
	
	function tagga(){
		var categoria = 'Contatto Form';
		var azione    = 'Submit';
		//var etichetteDisponibili = [
		//	'General', 'Sales', 'Support', 'Quote', 'Custom Light'
		//];
		var etichetta = $('input[name=enquiry]').val();
		//console.log(etichetta);
		var optional_value;
		
		//if($('input[name=sale_reference]').val() !== 0){
		//	optional_value=$('input[name=sale_reference]').val();					
		//}else{
		//	optional_value=window.location.href ;								
		//}
		
		if($('select[name=exhibition]').val() !== 0){ 
			optional_value = $(myformsubmit + ' .events option:selected').val();
		}else{
			optional_value = $(myformsubmit + ' .trade option:selected').val();	
		}
		
		gtag('event', azione, {
			'event_category': categoria,
			'event_label'   : etichetta,
			'value'         : optional_value
		});
		
		ga('send', 'event', categoria, azione, etichetta);
		console.log( categoria +' '+ azione +' '+ etichetta);
	}
    
</script>

												
